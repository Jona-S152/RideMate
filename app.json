{
  "expo": {
    "name": "RideMate",
    "slug": "RideMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TitleApp.png",
    "scheme": "ridemate",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicaci贸n para el viaje",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Necesitamos tu ubicaci贸n incluso con la app cerrada",
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "foregroundService": {
        "notificationTitle": "Viaje activo",
        "notificationBody": "Compartiendo ubicaci贸n en tiempo real"
      },
      "newArchEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7QUc9cGMHwVNILiyJjJc0yvM5KVBZsEA"
        }
      },
      "permissions": [
        "WAKE_LOCK",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/TitleApp.png",
        "backgroundColor": "#000D3A"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.RideMate",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que RideMate acceda a tu ubicaci贸n siempre.",
          "isAndroidForegroundServiceEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidLevelAndAboveForegroundServiceEnabled": true
        }
      ],
      "@rnmapbox/maps",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/TitleApp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000D3A"
        }
      ],
      "expo-maps"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4cab86f-ce79-4ee9-90ab-eaef88ce61a8"
      }
    },
    "splash": {
      "image": "./assets/images/TitleApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#000D3A"
    }
  }
}